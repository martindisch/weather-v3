{
  "name": "weather-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "vite build",
    "check": "prettier --plugin-search-dir . --check . && eslint . && svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "format": "prettier --plugin-search-dir . --write .",
    "migrations": "wrangler d1 migrations apply weather-v3 --local",
    "testdata": "node tools/generate-test-data.js && wrangler d1 execute DB --local --file testdata.sql",
    "pages": "vite build && wrangler pages dev"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20240320.1",
    "@databases/split-sql-query": "^1.0.4",
    "@databases/sql": "^3.3.0",
    "@sveltejs/adapter-cloudflare": "^4.2.0",
    "@sveltejs/kit": "^2.5.5",
    "@sveltejs/pancake": "^0.0.18",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "^9.4.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "prettier-plugin-svelte": "^3.2.2",
    "svelte": "^4.2.12",
    "svelte-check": "^3.6.8",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.6.2",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "wrangler": "^3.41.0"
  },
  "type": "module"
}
