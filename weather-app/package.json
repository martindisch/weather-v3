{
  "name": "weather-app",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --plugin-search-dir . --check . && eslint .",
    "format": "prettier --plugin-search-dir . --write .",
    "migrations": "wrangler d1 migrations apply weather-v3 --local",
    "pages": "vite build && wrangler pages dev .svelte-kit/cloudflare --local --persist --port 5173 --compatibility-date 2022-12-21"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@databases/split-sql-query": "^1.0.3",
    "@databases/sql": "^3.2.0",
    "@sveltejs/adapter-cloudflare-workers": "^1.0.0",
    "@sveltejs/kit": "^1.0.0",
    "@sveltejs/pancake": "^0.0.18",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "autoprefixer": "^10.4.13",
    "better-sqlite3": "^8.0.1",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.0",
    "prettier-plugin-svelte": "^2.8.1",
    "svelte": "^3.54.0",
    "svelte-check": "^2.9.2",
    "tailwindcss": "^3.2.4",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3",
    "vite": "^4.0.0",
    "wrangler": "^2.6.2"
  },
  "type": "module"
}
